{
  "name": "quantum-rpa",
  "private": true,
  "version": "0.0.0",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:react\" \"npm run dev:electron\"",
    "dev:react": "vite",
    "dev:electron": "npm run build:electron && wait-on tcp:5173 && cross-env NODE_ENV=development electron .",
    "build:electron": "tsc -p tsconfig.electron.json && tsc-alias -p tsconfig.electron.json",
    "watch:electron": "tsc -p tsconfig.electron.json -w",
    "build": "tsc && vite build && npm run build:electron && electron-builder",
    "preview": "vite preview"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "iconv-lite": "^0.7.2",
    "lucide-react": "^0.562.0",
    "module-alias": "^2.2.3",
    "mysql2": "^3.16.1",
    "node-firebird": "^1.1.9",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.12.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/sqlite3": "^3.1.11",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "tsc-alias": "^1.8.16",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "wait-on": "^9.0.3"
  },
  "_moduleAliases": {
    "@shared": "dist-electron/shared",
    "@preload": "dist-electron/preload",
    "@database": "dist-electron/main/database",
    "@services": "dist-electron/main/services",
    "@repositories": "dist-electron/main/repositories",
    "@controllers": "dist-electron/main/controllers",
    "@utils": "dist-electron/main/utils",
    "@constants": "dist-electron/main/constants",
    "@errors": "dist-electron/main/errors"
  }
}
